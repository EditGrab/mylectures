package lv.edite.player;

public class FootballPlayer {
	
	String name;
	String position;
	int shirtNumber;
	boolean build;
	long height;
	int weight;
	
	public String getName(){
		return name;
	}
	
	public String getPosition(){
		return position;
	}
	
	public int getShirtName(){
		return shirtNumber;
	}
	
	public boolean getBuild(){
		return build;
	}
	
	public long getHeight(){
		return height;
	}
	
	public int getWeight(){
		return weight;
	}
}

